
@model MVC_EF_Start.Models.Test.Result
<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">

<head>
    <meta charset="utf-8" />
    <title>Chart.js examples</title>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.3/Chart.js"></script>
</head>
<body>
    <h1> Bar chart example</h1>

    <canvas id="barchartexample" width="800" height="450"></canvas>

    <script>
    new Chart(document.getElementById("barchartexample"), {
      type: 'bar',
      data: {
        labels: [@Html.Raw(@Model.committee_id)],
        datasets: [
          {
            label: "Population (millions)",

            data: [@Model.contribution_receipt_amount]
          }
        ]
      },
      options: {
        legend: { display: false },
        title: {
          display: true,
          text: 'test'
        }
      }
    });
    </script>
</body>
</html>